@{var model = new DodgeDynasty.Mappers.Shared.ColorStylesMapper().GetModel(); }
<style type="text/css">
@foreach (var color in model.Colors)
{	<text>
	.@color.ClassName
	{
		border-color: @color.ColorValue;
	}</text>
}
@{var highlightModel = new DodgeDynasty.Mappers.Highlights.HighlightsMapper().GetModel(); }
@foreach (var highlight in highlightModel.Highlights)
{	<text>
	.highlighted.@highlight.HighlightClass
	{
		background-color: @highlight.HighlightValue;
	}</text>
}
</style>