@model DodgeDynasty.Models.DraftSetupModel
@{
	ViewBag.Title = "Setup Draft Picks";
	Model.AdminMode = DodgeDynasty.Shared.Constants.Modes.Commish;
}
@section scripts {
	<script type="text/javascript" src="@Url.Script("~/Scripts/SetupDraft.js")"></script>
	<script type="text/javascript">
		$(function () {
			adminMode = '@DodgeDynasty.Shared.Constants.Modes.Commish';
		});
	</script>
}
@Html.Partial(DodgeDynasty.Shared.Constants.Views.SetupDraftPartial, Model)